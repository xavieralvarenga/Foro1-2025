import java.util.HashMap;
import java.util.LinkedHashMap;
import java.util.TreeMap;


public class EjemplosDeMapas {
    public static void main(String[] args) {
    /*HashMap se declara de la siguente manera*/    
    HashMap<String, String> horarioClases = new HashMap<>();
    
    /*Para ingresar datos al HashMap utilizamos put*/
    horarioClases.put("Matemáticas", "Lunes");
    horarioClases.put("Ciencias", "Viernes");
    horarioClases.put("Sociales", "Jueves");
    horarioClases.put("Lenguaje", "Martes");
    horarioClases.put("Física", "Miercoles");
    
    /*Para imprimir los datos que contiene el HashMap, podemos usar: 
    System.out.println y le decimos que queremos que imprima nuestro HashMap*/
    System.out.println("Ejemplo de HashMap");
    System.out.println();
    System.out.println(horarioClases);
    
    /*para eliminar datos ingresaos podemos usar el metodo remove*/
    horarioClases.remove("Sociales");
    
     System.out.println(horarioClases);
     System.out.println("Fin de ejemplo de HashMap");
     System.out.println();
     
     
     
     
     LinkedHashMap<Integer, String> idCliente = new LinkedHashMap<>();
    
    /*Para ingresar datos al LinkedHashMap utilizamos put*/
    idCliente.put(1, "Roberto");
    idCliente.put(7, "Michelle");
    idCliente.put(2, "Uriel");
    idCliente.put(5, "Claudia");
    idCliente.put(3, "Rosa");
    
    /*Para imprimir los datos que contiene el LinkedHashMap, podemos usar: 
    System.out.println y le decimos que queremos que imprima nuestro LinkedHashMap*/
    
    System.out.println("Ejemplo de LinkedHashMap");
    System.out.println();
    System.out.println();System.out.println(idCliente);
    
    /*para eliminar datos ingresaos podemos usar el metodo remove.
    removeremos el valor a partir de la clave de ese par de datos*/
    idCliente.remove(7);
    
     System.out.println(idCliente);
     System.out.println("Fin de ejemplo de LinkedHashMap");
     System.out.println();
     
    
    TreeMap<Integer, String> listaProductos = new TreeMap<>();
    
    /*Para ingresar datos al TreeMap utilizamos put*/
    listaProductos.put(1, "Rodillo");
    listaProductos.put(7, "Lija");
    listaProductos.put(2, "Maneral");
    listaProductos.put(5, "Pegamento");
    listaProductos.put(3, "Tornillo");
    listaProductos.put(6, "Cubeta");
    listaProductos.put(4, "Brocha");
    
    /*Para imprimir los datos que contiene el TreeMap, podemos usar: 
    System.out.println y le decimos que queremos que imprima nuestro TreeMap*/
    System.out.println("Ejemplo de LinkedHashMap");
    System.out.println();
    System.out.println(listaProductos);
    
    /*para eliminar datos ingresaos podemos usar el metodo remove.
    removeremos el valor a partir de la clave de ese par de datos*/
    listaProductos.remove(7);
    
     System.out.println(listaProductos);
     System.out.println("Fin de ejemplo de TreeMap");
     System.out.println();
        
    }   
}
    
    
 
